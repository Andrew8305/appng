		<tag-class>org.appng.taglib.TagletAdapter</tag-class>
		<body-content>scriptless</body-content>
		<attribute>
			<name>application</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<name>method</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
